<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MostrarArbol"
   >

    <Button
        android:id="@+id/btnEliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-400dp"
        android:layout_marginTop="340dp"
        android:text="Eliminar" />

    <Button
        android:id="@+id/btnVolver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="340dp"
        android:layout_marginLeft="490dp"
        android:text="Volver" />

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="50dp">

        <ImageView
            android:id="@+id/img0.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img0.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img1.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="50dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img1.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img2.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="50dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img2.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img3.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="50dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img3.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />
    </androidx.gridlayout.widget.GridLayout>

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="-500dp">

        <ImageView
            android:id="@+id/img4.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img4.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img5.1"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="180dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />

        <ImageView
            android:id="@+id/img5.2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/fui_ic_anonymous_white_24dp" />
    </androidx.gridlayout.widget.GridLayout>




